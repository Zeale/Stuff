<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zeale.apps.stuff.app.guis.windows.calculator.calculators.statistics.DataInterpreterController">
   <content>
      <TabPane>
         <tabs>
            <Tab closable="false" text="Discrete Data">
               <content>
                  <ScrollPane fitToWidth="true">
                     <content>
                        <VBox alignment="TOP_CENTER" minHeight="500.0" minWidth="400.0" spacing="25.0">
                           <children>
                              <VBox alignment="CENTER">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 1.4em;" text="Data Input" />
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter data below. Separate each datum by some whitespace.">
                                       <font>
                                          <Font name="System Italic" size="12.0" />
                                       </font>
                                    </Text>
                                 </children>
                              </VBox>
                              <TextArea fx:id="discreteDataInput" prefHeight="150.0" prefWidth="300.0" wrapText="true" />
                              <VBox alignment="CENTER">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 1.4em;" text="Data Output" />
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Each detected datum is shown in the box below.">
                                       <font>
                                          <Font name="System Italic" size="12.0" />
                                       </font>
                                    </Text>
                                 </children>
                              </VBox>
                              <FlowPane fx:id="discreteDetectedData" hgap="8.0" style="-fx-border-color: -stuff-dark;" vgap="8.0">
                                 <padding>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </padding>
                              </FlowPane>
                              <Button mnemonicParsing="false" onAction="#calcDiscStats" styleClass="pop-button" text="Calculate Statistics" />
                              <TableView fx:id="discreteTable">
                                <columns>
                                  <TableColumn fx:id="discreteProps" minWidth="100.0" prefWidth="130.0" text="Property" />
                                  <TableColumn fx:id="discreteVals" minWidth="150.0" prefWidth="596.0" text="Value" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                           <padding>
                              <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                           </padding>
                        </VBox>
                     </content>
                  </ScrollPane>
               </content>
            </Tab>
            <Tab closable="false" text="Continuous Data">
               <content>
                  <ScrollPane fitToWidth="true">
                     <content>
                        <VBox alignment="TOP_CENTER" minHeight="500.0" minWidth="400.0" spacing="25.0">
                           <children>
                              <VBox alignment="CENTER">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 1.4em;" text="Data Input" />
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter data below. Separate each datum by some whitespace.">
                                       <font>
                                          <Font name="System Italic" size="12.0" />
                                       </font>
                                    </Text>
                                 </children>
                              </VBox>
                              <TextArea fx:id="continuousDataInput" prefHeight="150.0" prefWidth="300.0" wrapText="true" />
                              <VBox alignment="CENTER">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 1.4em;" text="Data Output" />
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Each detected datum is shown in the box below.">
                                       <font>
                                          <Font name="System Italic" size="12.0" />
                                       </font>
                                    </Text>
                                 </children>
                              </VBox>
                              <FlowPane fx:id="continuousDetectedData" hgap="8.0" style="-fx-border-color: -stuff-dark;" vgap="8.0">
                                 <padding>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </padding>
                              </FlowPane>
                              <Button mnemonicParsing="false" onAction="#calcContStats" styleClass="pop-button" text="Calculate Statistics" />
                              <TableView fx:id="continuousTable">
                                 <columns>
                                    <TableColumn fx:id="continuousProps" minWidth="100.0" prefWidth="130.0" text="Property" />
                                    <TableColumn fx:id="continuousVals" minWidth="150.0" prefWidth="604.0" text="Value" />
                                 </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                           <padding>
                              <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
                           </padding>
                        </VBox>
                     </content>
                  </ScrollPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </content>
</ScrollPane>
